pipeline{
    agent any

    tools{
        jdk 'java17'
        maven 'maven3'
    }
  
    stages {
        stage('Workspace Cleaning') {
            steps {
                cleanWs()
            }
        }
    }
    stages {
        stage('Git SCM') {
            steps {
                git branch: 'main', url: 'https://github.com/Skywalker4123/complete-prodcution-e2e-pipeline.git'
            }
        }
    }        
}